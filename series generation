 #include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int a=7,b=5;
    for(int i=0;i<n;i++)
    {
     if(i%2==0)
        {
          printf("%d ",a);
          a++;
        }
      else
       {
        printf("%d ",b);
        b++;
       }
   }
}
 
nput (stdin)

5
Your Output (stdout)

7 5 8 6 9 
Expected Output

7 5 8 6 9
