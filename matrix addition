#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int m,n;
    scanf("%d%d",&m,&n);
        int a[m][n],b[m][n],c[m][n],i,j;
        for(i=0;i<m;i++)
        {   
            for(j=0;j<n;j++)
            {
              scanf("%d",&a[i][j]);
        }
            }
    for(i=0;i<m;i++)
    {
      for(j=0;j<n;j++)  
      {
        scanf("%d",&b[i][j]);  
      }
    }
     for(i=0;i<m;i++)
        {
            for(j=0;j<n;j++)
            {
            c[i][j]=a[i][j]+b[i][j];
             printf("%d\t",c[i][j]);
            }
         printf("\n");
        }
    return 0;
        }

    
