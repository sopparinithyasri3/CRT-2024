#include <stdio.h>
#include<stdlib.h>
void swap(int x,int y)
{
    int t;
    t=x;
    x=y;
    y=t;
}
int main()
{
int a,b;
printf("enter two numbers");
scanf("%d%d",&a,&b);
printf("values before swapping a=%d b=%d\n",a,b);
swap(a,b);
printf("values after swapping a=%d b=%d\n",b,a);
}
enter two numbers3
4
values before swapping a=3 b=4
values after swapping a=4 b=3
